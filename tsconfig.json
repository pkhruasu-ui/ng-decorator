{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "lib": [
      "es2015",
      "dom"
    ],
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": false,
    "noImplicitAny": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [],
    "moduleResolution": "node",
    "pretty": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "files": [
    "./src/decorators.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "angularCompilerOptions": {
    "genDir": "./distlib",
    "debug": false,
    "skipTemplateCodegen": true,
    "skipMetadataEmit": false,
    "strictMetadataEmit": true
  }
}